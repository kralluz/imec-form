    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cb2a1928-6de4-4283-85ee-a426e2ad89b3"
      }
    }
  }
}
